				<key>script</key>
				<string>#!/usr/bin/env bash
CACHE="$HOME/Library/Application Support/Microsoft Edge/Default/Workspaces/WorkspacesCache"

# optional: let Alfred pass the query as $1 to filter names
QUERY="${1,,}"   # lower-case query

jq --arg q "$QUERY" '
{
  items: [
    .workspaces[]
    | select(.name|ascii_downcase|contains($q))
    | {uid: .id, title: .name, arg: .id}
  ]
}
' "$CACHE"
</string>
